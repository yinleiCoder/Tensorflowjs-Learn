ript.js","original":{"line":49,"column":36}},{"generated":{"line":61,"column":40},"source":"script.js","original":{"line":49,"column":33}},{"generated":{"line":61,"column":41},"source":"script.js","original":{"line":49,"column":43}},{"generated":{"line":61,"column":42},"source":"script.js","original":{"line":49,"column":44}},{"generated":{"line":61,"column":43},"source":"script.js","original":{"line":49,"column":43}},{"generated":{"line":61,"column":44},"source":"script.js","original":{"line":49,"column":33}},{"generated":{"line":61,"column":45},"source":"script.js","original":{"line":49,"column":19}},{"generated":{"line":61,"column":46},"source":"script.js","original":{"line":49,"column":4}},{"generated":{"line":61,"column":47},"source":"script.js","original":{"line":8,"column":28}},{"generated":{"line":61,"column":48},"source":"script.js","original":{"line":49,"column":50}},{"generated":{"line":62,"column":0},"source":"script.js","original":{"line":50,"column":4}},{"generated":{"line":63,"column":0},"source":"script.js","original":{"line":51,"column":4}},{"name":"alert","generated":{"line":65,"column":0},"source":"script.js","original":{"line":52,"column":4}},{"name":"alert","generated":{"line":65,"column":2},"source":"script.js","original":{"line":52,"column":4}},{"generated":{"line":65,"column":7},"source":"script.js","original":{"line":52,"column":9}},{"generated":{"line":65,"column":8},"source":"script.js","original":{"line":52,"column":11}},{"name":"output","generated":{"line":65,"column":29},"source":"script.js","original":{"line":52,"column":31}},{"generated":{"line":65,"column":35},"source":"script.js","original":{"line":52,"column":37}},{"name":"dataSync","generated":{"line":65,"column":36},"source":"script.js","original":{"line":52,"column":38}},{"generated":{"line":65,"column":44},"source":"script.js","original":{"line":52,"column":31}},{"generated":{"line":65,"column":47},"source":"script.js","original":{"line":52,"column":49}},{"generated":{"line":65,"column":48},"source":"script.js","original":{"line":52,"column":31}},{"generated":{"line":65,"column":49},"source":"script.js","original":{"line":52,"column":52}},{"generated":{"line":65,"column":51},"source":"script.js","original":{"line":52,"column":9}},{"generated":{"line":65,"column":52},"source":"script.js","original":{"line":52,"column":4}},{"generated":{"line":66,"column":0},"source":"script.js","original":{"line":53,"column":1}},{"generated":{"line":66,"column":1},"source":"script.js","original":{"line":8,"column":0}}],"sources":{"script.js":"// 线性回归\r\n\r\nimport * as tf from '@tensorflow/tfjs'\r\nimport * as tfvis from '@tensorflow/tfjs-vis'\r\nimport { outerProduct } from '@tensorflow/tfjs';\r\n// 可视化库：@tensorflow/tfjs-vis\r\n// 用parcel打包工具运行：parcel + 文件夹名\r\nwindow.onload = async () => {\r\n    const xs = [1,2,3,4];\r\n    const ys = [1,3,5,7];\r\n\r\n    // 散点图\r\n    tfvis.render.scatterplot(\r\n        {name: '线性回归训练集'},\r\n        { values: xs.map((x,i) => ({x, y: ys[i]})) },\r\n        { xAxisDomain: [0,5], yAxisDomain: [0,8]  }\r\n    );\r\n\r\n    // 定义单层单个神经元组成的神经网络：\r\n    // 1。初始化一个神经网络模型\r\n    // 2。 为神经网络模型添加层\r\n    // 3。 设计层的神经元个数和inputShape\r\n    const model = tf.sequential();\r\n    model.add(tf.layers.dense({units: 1, inputShape: [1]}));\r\n\r\n    // 损失函数： 均方误差MSE\r\n    // 优化器： 随机梯度下降SGD\r\n    model.compile({loss: tf.losses.meanSquaredError, optimizer:tf.train.sgd(0.1)});\r\n    \r\n    // 训练模型及可视化训练过程：\r\n    // 1 将训练数据转为Tensor\r\n    // 2 训练模型\r\n    // 3 使用tfvis可视化训练过程\r\n    const inputs = tf.tensor(xs);\r\n    const labels = tf.tensor(ys);\r\n    await model.fit(inputs, labels,{\r\n        batchSize: 4,\r\n        epochs: 100,\r\n        callbacks: tfvis.show.fitCallbacks(\r\n            {name: '训练过程'},\r\n            ['loss']\r\n        )\r\n    });\r\n\r\n    // 进行预测：\r\n    // 1. 将待预测数据转为Tensor\r\n    // 2. 使用训练好的模型进行预测\r\n    // 3. 将输出的Tensor转为普遍数据并显示\r\n    const output = model.predict(tf.tensor([5])); // 预测输入5，结果tensor是多少\r\n    // output.print();\r\n    // output.dataSync[0];//tensor转化为数字\r\n    alert(`如果x = 5, 那么预测的y = ${output.dataSync()[0]}`)\r\n};\r\n\r\n"},"lineCount":null}},"error":null,"hash":"7ef0bd5586f99739e642375d6e32afbf","cacheData":{"env":{}}}